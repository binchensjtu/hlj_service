
#head {
    text-align: center;
}


img {max-width: 100%;}

.progress li {
    display: inline-block;

}

.progress li span {
    border-radius: 50%;
}

.nod_msg { background: #fcc; display: block; padding: 2px 5px; margin-left: 5px; border-radius: 5px; }
.nod_msg {
    font-weight: bold;
}
.help-block {
    display:block;
    margin-bottom:10px;
    clear: both;
}

.clearfix {

    *zoom: 1;

}



.clearfix:before,

.clearfix:after {

    display: table;

    line-height: 0;

    content: "";

}



.clearfix:after {

    clear: both;

}